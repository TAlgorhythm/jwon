-- 코드를 입력하세요
SELECT A.CAR_ID AS CAR_ID
FROM CAR_RENTAL_COMPANY_CAR A JOIN CAR_RENTAL_COMPANY_RENTAL_HISTORY B
ON (A.CAR_ID = B.CAR_ID)
WHERE MONTH(B.START_DATE) = 10 AND A.CAR_TYPE LIKE '%세단%'
GROUP BY A.CAR_ID
ORDER BY A.CAR_ID DESC;


# SELECT * FROM CAR_RENTAL_COMPANY_CAR A JOIN CAR_RENTAL_COMPANY_RENTAL_HISTORY B ON (A.CAR_ID = B.CAR_ID)